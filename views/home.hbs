<div class="container-fluid outer-container position-relative">
    <div class="suggested-movie">
        <div class="overlay-top"></div>
        <img src="https://image.tmdb.org/t/p/original/1pdfLvkbY9ohJlCjQH2CZjjYVvJ.jpg" alt="">
        <div class="overlay-bottom"></div>
    </div>

    <div>   
        <h2>Popular Movies</h2>
        <div id="carouselExampleIndicators" class="carousel slide">
            <div class="carousel-inner full-carousel">
                {{#each popular_movies}}
                {{#if this.checkFirst}}
                <div class="carousel-item active">
                    <div class="d-flex justify-content-center align-items-center place-6-movies">
                        {{#each this.list6Movies}}
                        <div class="movie-hoverred ms-1">
                            <img src="https://image.tmdb.org/t/p/original/{{this.poster_path}}"
                                class="d-block rounded-top" alt="...">
                            <div class="movie-actions bg-black p-2 d-none rounded-bottom">
                                <div class="option-list d-flex">
                                    <div
                                        class="bg-light rounded-circle me-2 d-flex justify-content-center align-items-center play-video">
                                        <i class="fa-solid fa-play fs-3 text-dark"></i>
                                    </div>
                                    <div
                                        class="bg-dark rounded-circle me-2 d-flex justify-content-center align-items-center border border-5">
                                        <i class="fa-solid fa-plus fs-3"></i>
                                    </div>
                                    <div
                                        class="bg-dark rounded-circle me-2 d-flex justify-content-center align-items-center border border-5">
                                        <i class="fa-solid fa-thumbs-up fs-3"></i>
                                    </div>
                                    <div
                                        class="bg-dark rounded-circle d-flex justify-content-center align-items-center ms-auto border border-5 open-movie-details">
                                        <i class="fa-solid fa-chevron-down fs-3"></i>
                                    </div>
                                </div>
                                <p class="text-success fs-5 fw-bold m-0 mt-3">82% Match</p>
                                <div class="d-flex justify-content-start align-items-center m-0 small-info-of-movie">
                                    <div class="border border-3 m-0">
                                        {{#if this.adult}}
                                        <p class="m-0 ps-2 pe-2">18+</p>
                                        {{else}}
                                        <p class="m-0 ps-2 pe-2">13+</p>
                                        {{/if}}
                                    </div>
                                    <p class="m-0 fs-5 ms-2"></p>{{this.runtime}}</p>
                                    <div class="border border-2 rounded m-0 ms-2">
                                        <p class="m-0 ps-1 pe-1 fw-bold">HD</p>
                                    </div>
                                    <div class="m-0 ms-2 d-flex align-items-center">
                                        <i class="fa-solid fa-podcast fs-3"></i>
                                        <div>
                                            <p class="fs-5 fw-bold m-0 ms-1">Spatial</p>
                                            <p class="m-0 ms-1 lh-1">Audio</p>
                                        </div>
                                    </div>
                                </div>

                                <div
                                    class="d-flex justify-content-start flex-wrap mt-3 align-items-center movie-categories">
                                    {{#each this.genres}}
                                    {{#if this.isFirst}}
                                    <p class="text-light m-0">{{this.name}}</p>
                                    {{else}}
                                    <div class="m-0 bg-secondary rounded ms-2"></div>
                                    <p class="text-light m-0 ms-2">{{this.name}}</p>
