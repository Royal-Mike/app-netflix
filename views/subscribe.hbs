<div class="container">
  <h1>Subscribe to Our Services</h1>
  <p>Enjoy unlimited access to movies, TV shows, and more!</p>
  <div class="subscription-options">
    <button id="subscribe-btn" class="btn btn-primary">Subscribe for 30 Days</button>
    <button id="trial-btn" class="btn btn-secondary">Try for 7 Days</button>
  </div>
</div>

<script>
  document.getElementById('subscribe-btn').addEventListener('click', function() {
    window.location.href = '/subscribe/process';
  });

  document.getElementById('trial-btn').addEventListener('click', function() {
    window.location.href = '/subscribe/trial';
  });
</script>
